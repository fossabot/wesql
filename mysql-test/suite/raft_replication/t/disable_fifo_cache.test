use test;
create table t1 (data longblob);

--disable_query_log
let $counter= 20;
while ($counter)
{
  insert into t1 values (repeat('a', 2*1024*1024));
  dec $counter;
}
--enable_query_log
# check replicate log success
--sleep 5
select count(distinct(match_index)) from information_schema.wesql_cluster_global;
drop table t1;
