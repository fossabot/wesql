--source suite/smartengine/include/have_smartengine.inc
set global smartengine_strict_collation_check = off;
--disable_warnings
DROP TABLE IF EXISTS t1;
--enable_warnings

CREATE TABLE t1(c1 int primary key, c2 mediumblob) ENGINE = SMARTENGINE;

--disable_warnings
INSERT INTO t1 values (1, repeat('a', 16777216));
--enable_warnings

set global smartengine_flush_memtable = on;
sleep 5;

--echo restarting...
--source include/restart_mysqld.inc

SELECT c1, MD5(c2) FROM t1;

set global smartengine_compact = 55834574848;
sleep 5;

--echo restarting...
--source include/restart_mysqld.inc

SELECT c1, MD5(c2) FROM t1;

DROP TABLE t1;
--source suite/smartengine/include/check_smartengine_log_error.inc
