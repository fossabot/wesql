--source suite/smartengine/include/have_smartengine.inc

create table t1 (
  id int, 
  value int, 
  primary key (id)
) ENGINE = SMARTENGINE;

insert into t1 values(1,1),(2,2);
set autocommit=0;

begin;
insert into t1 values (50,50);
select * from t1;

update t1 set id=id+100;

select * from t1;

rollback;
set autocommit=1;

drop table t1;
