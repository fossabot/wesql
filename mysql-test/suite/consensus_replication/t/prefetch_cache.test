use test;
create table t1 (data longblob);

--disable_query_log
let $counter= 5;
while ($counter)
{
  insert into t1 values (repeat('a', 5*1024*1024));
  dec $counter;
}
insert into t1 select * from t1;
insert into t1 values (repeat('a', 21*1024*1024));
let $counter= 5;
while ($counter)
{
  insert into t1 values (repeat('a', 1024));
  dec $counter;
}
--enable_query_log

