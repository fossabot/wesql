CREATE TABLE t1 (id1 INT, id2 INT, id3 INT,
                 PRIMARY KEY (id1, id2, id3),
                 UNIQUE KEY (id3, id1)) ENGINE=SMARTENGINE;

CREATE TABLE t2 (id1 INT, id2 INT, id3 INT,
                 PRIMARY KEY (id1, id2, id3),
                 UNIQUE KEY (id3, id1) COMMENT 'cf') ENGINE=SMARTENGINE;


