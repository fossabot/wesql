DROP TABLE IF EXISTS t1;
CREATE TABLE t1 (
pk INT AUTO_INCREMENT PRIMARY KEY,
c1 int,
c2 varchar(100),
b JSON,
b1 JSON not null,
b17m BLOB(17000000),
t TINYBLOB,
m MEDIUMBLOB,
l LONGBLOB,
n JSON not null,
c3 varchar(100),
c4 int
) ENGINE = SMARTENGINE;
