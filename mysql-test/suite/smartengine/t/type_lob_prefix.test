--source suite/smartengine/include/have_smartengine.inc

set global smartengine_strict_collation_check=1;
#============================
# character-set utf8mb4
#============================
--disable_warnings
set names utf8mb4;

#===================
#prefix-blob-type
#===================
drop table if exists tt;
create table tt(id int, c1 blob) ENGINE = SMARTENGINE charset=utf8mb4 collate utf8mb4_general_ci;

alter table tt add unique index idx_uk(c1(2));

insert into tt values(1,'a中');
insert into tt values(1,'a国');
--error 1062
insert into tt values(1,'a中');

insert into tt values(1,'aabb');

--error 1062
insert into tt values(1,'aa');
insert into tt values(1,'AAcc');


drop table tt;
create table tt(id int, c1 blob) ENGINE = SMARTENGINE charset=utf8mb4 collate utf8mb4_0900_ai_ci;

alter table tt add unique index idx_uk(c1(2));

insert into tt values(1,'a中');
insert into tt values(1,'a国');
--error 1062
insert into tt values(1,'a中');

insert into tt values(1,'aabb');

--error 1062
insert into tt values(1,'aa');
insert into tt values(1,'AAcc');


drop table tt;
create table tt(id int, c1 blob) ENGINE = SMARTENGINE charset=utf8mb4 collate utf8mb4_bin;

alter table tt add unique index idx_uk(c1(2));

insert into tt values(1,'a中');
insert into tt values(1,'a国');
--error 1062
insert into tt values(1,'a中');

insert into tt values(1,'aabb');

--error 1062
insert into tt values(1,'aacc');
insert into tt values(1,'aAcc');
select * from tt;

