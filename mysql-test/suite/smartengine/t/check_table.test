--source check_table.inc

DROP TABLE IF EXISTS t2;
CREATE TABLE t2 (
    id INT,
    data CHAR(20),
    PRIMARY KEY(data(7)),
    KEY idx_data(data(10)),
    UNIQUE KEY idx_data2(data))
ENGINE=SMARTENGINE,
COLLATE=utf8mb4_general_ci;

INSERT INTO t2 VALUES(1, '123456');
INSERT INTO t2 VALUES(2, '123456982357894275');

CHECK TABLE t2;

DROP TABLE t2;
